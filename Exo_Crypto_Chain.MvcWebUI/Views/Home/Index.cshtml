                <div class="card-body">
                    <h5 class="card-title">@urun.Name</h5>
                    <p class="card-text">@urun.Description</p>
                </div>
                <div class="card-footer">

                    <small class="text-muted">@urun.Price</small>
                </div>
            </div>
        </div>



